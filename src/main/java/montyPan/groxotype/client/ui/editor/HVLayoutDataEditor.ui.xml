<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:f="urn:import:com.sencha.gxt.widget.core.client.form" 
	xmlns:core="urn:import:com.sencha.gxt.widget.core.client" 
	xmlns:c="urn:import:com.sencha.gxt.widget.core.client.container">
	
	<core:FramedPanel ui:field="root" width="300" height="85">
		<c:VerticalLayoutContainer>
			<f:FieldLabel text="width" labelWidth="50">
				<f:widget>
					<f:DoubleField ui:field="width" allowBlank="false" />
				</f:widget>
			</f:FieldLabel>
			
			<f:FieldLabel text="height" labelWidth="50">
				<f:widget>
					<f:DoubleField ui:field="height" allowBlank="false" />
				</f:widget>
			</f:FieldLabel>
		</c:VerticalLayoutContainer>
	</core:FramedPanel>
</ui:UiBinder> 